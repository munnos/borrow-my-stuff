turn;\n      \n    \n\n   \n\n  return (\n    \n    <div className=\"container\">\n    <Row>\n        <Card className=\"text-center bg-secondary text-white my-5 py-4\">\n          <Card.Title>Welcome to your Dashboard{/*  {user.name}*/}</Card.Title> \n        </Card>\n      </Row> \n\n      <Row className=\"px-4 my-2\">\n        <Col sm={3}>\n          <div className=\"d-grid gap-2\">\n            <Button variant=\"primary\" onClick={handleClick} value=\"requested\" size=\"lg\"style={{backgroundColor:'#253F41'}}>\n              Requested items\n            </Button>\n            <Button variant=\"secondary\" onClick={handleClick} value=\"listed\" size=\"lg\" style={{backgroundColor:'#253F41'}}>\n              Listed items\n            </Button>\n            <Button variant=\"secondary\" onClick={handleClick} value =\"liked\" size=\"lg\" style={{backgroundColor:'#253F41'}}>\n              Liked items\n            </Button>\n           \n            <Button variant=\"secondary\" href=\"/orderHistory\"size=\"lg\" style={{backgroundColor:'#253F41'}}>\n              View order history \n            </Button>\n              \n          </div>\n        </Col>\n        <Col sm={8}>\n          {/* components are called here */}\n\n        {profileView && testing(profileView)}\n\n\n        </Col>\n\n\n\n\n      </Row>\n       {testing}\n      <Cart />\n      \n    </div>\n  );\n};\n\nexport default UserDash;","C:\\Users\\Debra\\Documents\\BhamCodingBootcamp\\projectThree\\borrow-my-stuff\\client\\src\\utils\\GlobalState.js",[],"C:\\Users\\Debra\\Documents\\BhamCodingBootcamp\\projectThree\\borrow-my-stuff\\client\\src\\pages\\Detail.js",[],"C:\\Users\\Debra\\Documents\\BhamCodingBootcamp\\projectThree\\borrow-my-stuff\\client\\src\\pages\\AddListItem.js",[],"C:\\Users\\Debra\\Documents\\BhamCodingBootcamp\\projectThree\\borrow-my-stuff\\client\\src\\pages\\Home.js",["161"],"import React from \"react\";\nimport { Button, Image } from \"react-bootstrap\";\nimport Cart from \"..