ield]: value,\n    });\n  };\n\n  const handleSubmit = async (e) => {\n    e.preventDefault();\n    console.log(form);\n\n    const token = Auth.loggedIn() ? Auth.getToken() : null;\n\n    if (form !== {} && token) {\n      console.log(\"Items in the form\");\n      console.log(\"user logged in\");\n      console.log(form);\n      //find category id\n\n      const duration = form.duration;\n\n      console.log(duration);\n      // console.log(id);\n      // console.log(listingProduct);\n\n      try {\n        const { data } = await requestAProduct({\n          variables: {\n            // listingProduct,\n            duration,\n          },\n        });\n      } catch (err) {\n        console.error(err);\n      }\n      //reset form fields\n      setForm({ duration: \"\" });\n    }\n    //console.log(token);\n    if (!token) {\n      return false;\n    }\n  };\n  // if (!products.length) {\n  //   return <h3>No products yet</h3>;\n\n  // }\n  // console.log(products);\n  return (\n    <Container>\n      <h1>{props.title} </h1>\n      {props.products.getListedProductsByCategory &&\n        props.products.getListedProductsByCategory.map((product) => (\n          <Card\n            key={product}\n            border=\"dark\"\n            style={{\n              width: \"80vw\",\n              height: \"30vh\",\n              marginBottom: \"2rem\",\n              marginTop: \"2rem\",\n            }}\n          >\n            <Card.Header>{product.name}</Card.Header>\n            <Form>\n              <Form.Group controlId>\n                <Form.Label>Select a listing duration:</Form.Label>\n                <Form.Select\n                  value={form.duration}\n                  placeholder=\"Listing Duration\"\n                  onChange={(e) => {\n                    setField(\"duration\", e.target.value);\n                  }}\n                >\n                  <option>Select duration </option>\n                  <option value=\"1 week\">1 week </option>\n        