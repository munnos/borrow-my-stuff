gories from \"../components/ListingCategories\";\nimport Container from \"react-bootstrap/Container\";\n\n\n\nconst ViewListingsByCategory = () => {\n  let { id } = useParams(); \n  const { loading, data } = useQuery(QUERY_LISTEDCATEGORIES, { variables: { category: id}});\n\n  const productsByListedCategory = data || [];\n  console.log(data);\n\n  return (\n    <main>\n    <Container>\n          {loading ? (\n            <div>Loading...</div>\n          ) : (\n            <ListingCategories\n              products={productsByListedCategory}\n              title=\"Here is the list of products in your chosen category\"\n            />\n          )}\n      </Container>\n    </main>\n  );\n};\n\nexport default ViewListingsByCategory;\n","/Users/amalhirsi/Desktop/projects/borrow-my-stuff/client/src/pages/Success.js",[],"/Users/amalhirsi/Desktop/projects/borrow-my-stuff/client/src/pages/userDash.js",["176","177","178"],"\n\nimport Cart from \"../components/Cart\";\nimport Requested from \"../components/Requested\"\nimport Listed from \"../components/Listed\"\nimport Liked from \"../components/Liked.js\"\nimport { Button } from 'react-bootstrap' \n//import User from \"../../../server/models/User\";\nimport { Link } from 'react-router-dom';\nimport React, { useState } from 'react';\nimport Container from 'react-bootstrap/Container';\nimport Row from 'react-bootstrap/Row';\nimport Col from 'react-bootstrap/Col';\nimport CategoryCards from \"../components/CategoryCards\";\nimport Card from 'react-bootstrap/Card';\n\n//need logged in user id\n//user query for name - could be stored in local storage from shop functionality\n\n\nconst UserDash = () => {\n\n  const [profileView, setProfileView] = useState(\"\");\n\n  const handleClick = (event) => {\n    event.preventDefault();\n    setProfileView(event.target.value);\n\n    console.log('value is:', event.target.value);\n  \n    // ðŸ‘‡ï¸ value of input field\n    console.log('handleClick ðŸ‘‰ï¸',  event.target.value);\n    //testing(event.target.value);\n  }\n\n   const testing = (profileView) =>{\n    console.log('profileView:', profileView)\n      if(profileView === \"requested\") {\n        return   <Requested profileView={profileView}/>\n      } \n\n      if (profileView === \"listed\"){\n        return <Listed profileView={profileView}/>\n      } \n\n      if (profileView === \"liked\" ){\n        return  <Liked profileView={profileView}/>\n      } \n\n        console.log(\"no matching condition\");\n      }   \n\n    // switch(profileView) {\n    //   case \"requested\":\n    //     console.log(`inside switch statement ${profileView}`);\n    //     return <Requested profileView={profileView}/>\n    //     case \"listed\":\n    //       return <Listed profileView={profileView}/> \n    //       case \"liked\":\n    //         return <Liked profileView={profileView}/> \n    //         default:\n    //           console.log(\"not match\")\n    //           return;\n      \n    \n\n   \n\n  return (\n    \n    <div className=\"container\">\n    <Row>\n        <Card className=\"text-center bg-secondary text-white my-5 py-4\">\n          <Card.Title>Welcome to your Dashboard{/*  {user.name}*/}</Card.Title> \n        </Card>\n      </Row> \n\n      <Row className=\"px-4 my-2\">\n        <Col sm={3}>\n          <div className=\"d-grid gap-2\">\n            <Button variant=\"primary\" onClick={handleClick} value=\"requested\" size=\"lg\"style={{backgroundColor:'#253F41'}}>\n              Requested items\n            </Button>\n            <Button variant=\"secondary\" onClick={handleClick} value=\"listed\" size=\"lg\" style={{backgroundColor:'#253F41'}}>\n              Listed items\n            </Button>\n            <Button variant=\"secondary\" onClick={handleClick} value =\"liked\" size=\"lg\" style={{backgroundColor:'#253F41'}}>\n              Liked items\n            </Button>\n           \n            <Button variant=\"secondary\" href=\"/orderHistory\"size=\"lg\" style={{backgroundColor:'#253F41'}}>\n              View order history \n            </Button>\n              \n          </div>\n        </Col>\n        <Col sm={8}>\n          {/* components are called here */}\n\n        {profileView && testing(profileView)}\n\n\n        </Col>\n\n\n\n\n      </Row>\n       {testing}\n      <Cart />\n      \n    </div>\n  );\n};\n\nexport default UserDash;","/Users/amalhirsi/Desktop/projects/borrow-my-stuff/client/src/pages/ListedProduct.js",["179","180"],"import React from \"react\";\nimport Card from 'react-bootstrap/Card';\nimport { Button, Container } from 'react-bootstrap';\nimport { useQuery } from '@apollo/client';\nimport { useParams } from 'react-router-dom';\nimport { LISTED_PRODUCTS } from '../utils/queries';\nimport OneListedProduct from \"../components/OneListedProduct\";\nimport RequestToBorrow from \"../components/RequestToBorrow\";\n\nconst ListedProduct =  () => {\n    // queries are run here\n    // 1. get an id to run a query to get an item ?\n    const { id } = useParams(); // this comes from another component\n    console.log(id);\n    const { loading, data } = useQuery(LISTED_PRODUCTS, { variables: {  id }, });\n    const item = data?.getListedProductById||{};\n  console.log(\"inside request to borrow\");\n       \n  return (\n          <Container>\n          {loading ? (\n            <div>Loading data..</div>\n          ) : (\n            <OneListedProduct\n              item = { item }\n              title =\"One listed product\"\n            />      \n           )}\n            <RequestToBorrow\n              product = { id }\n              title =\"One listed product\"\n            />\n          </Container>\n         \n        );\n      }\n    export default  ListedProduct ;\n    \n","/Users/amalhirsi/Desktop/projects/borrow-my-stuff/client/src/utils/GlobalState.js",[],"/Users/amalhirsi/Desktop/projects/borrow-my-stuff/client/src/components/Liked.js",[],"/Users/amalhirsi/Desktop/projects/borrow-my-stuff/client/src/components/Navbar/index.js",[],"/Users/amalhirsi/Desktop/projects/borrow-my-stuff/client/src/components/Footer/index.js",[],"/Users/amalhirsi/Desktop/projects/borrow-my-stuff/client/src/utils/mutations.js",[],"/Users/amalhirsi/Desktop/projects/borrow-my-stuff/client/src/utils/auth.js",[],"/Users/amalhirsi/Desktop/projects/borrow-my-stuff/client/src/utils/queries.js",[],"/Users/amalhirsi/Desktop/projects/borrow-my-stuff/client/src/utils/actions.js",[],"/Users/amalhirsi/Desktop/projects/borrow-my-stuff/client/src/utils/helpers.js",[],"/Users/amalhirsi/Desktop/projects/borrow-my-stuff/client/src/components/Requested.js",["181","182","183"],"import React from \"react\";\nimport { useQuery } from \"@apollo/client\";\nimport { GET_MY_REQUESTED_ITEMS } from \"../utils/queries\";\nimport Card from \"react-bootstrap/Card\";\nimport Button from \"react-bootstrap/Button\";\nimport Row from \"react-bootstrap/Row\";\nimport Col from \"react-bootstrap/Col\";\nimport Container from \"react-bootstrap/Container\";\n\n//approve: false, active: true -> request is active - pending approval - default\n//approve: true, active: false -> request is no longer active - request approved\n//approve: false, active: false -> request is no longer active - request denied\n\nfunction Requested({ profileView }) {\n  console.log(\"inside request component\");\n  const { loading, data } = useQuery(GET_MY_REQUESTED_ITEMS);\n  const requestedProducts = data?.getRequestsIMade || [];\n  console.log(data);\n  console.log(requestedProducts);\n\n  if (!requestedProducts.length) {\n    return <h3>No Requested Products Yet</h3>;\n  }\n\n  return (\n    <div>\n      <Container fluid>\n        <Row className=\"justify-content-md-center\">\n          {requestedProducts &&\n            requestedProducts.map((requestedProduct) => {\n              if (\n                requestedProduct.approved === false &&\n                requestedProduct.active === true\n              ) {\n                return (\n                  <>\n                    <Col>\n                      <Card\n                        border=\"warning\"\n                        id=\"reqProdCard\"\n                        style={{ width: \"22rem\" }}\n                        key={requestedProduct._id}\n                      >\n                        <Card.Header\n                          as=\"h4\"\n                          className=\"text-center fw-bold bg-warning text-black\"\n                        >\n                          {\" \"}\n                          Pending approval...\n                        </Card.Header>\n                        <Card.Img\n                          id=\"reqProdImg\"\n                          variant=\"bottom\"\n                          src={requestedProduct.listingProduct.image}\n                        />\n                        <Card.Body className=\"bg-secondary fw-bold\">\n                          <Card.Title className=\"text-center fw-bold\">\n                            {requestedProduct.listingProduct.name}\n                          </Card.Title>\n                          {/* <Card.Title>Posted by {requestedProduct.listingProduct.user.FirstName}</Card.Title> */}\n                        </Card.Body>\n                        <Card.Footer\n                          as=\"h6\"\n                          className=\"cardFooter  bg-dark text-white\"\n                        >\n                          Requested on {requestedProduct.dateRequested}\n                          <br/> for {requestedProduct.duration}\n                        </Card.Footer>\n                      </Card>\n                    </Col>\n                  </>\n                );\n              } else if (\n                requestedProduct.approved === true &&\n                requestedProduct.active === false\n              ) {\n                return (\n                  <>\n                    <Col>\n                      <Card\n                        border=\"sucess\"\n                        id=\"reqProdCard\"\n                        style={{ width: \"22rem\" }}\n                        key={requestedProduct._id}\n                      >\n                        <Card.Header\n                          as=\"h5\"\n                          className=\"text-center fw-bold bg-success text-white\"\n                        >\n                          {\" \"}\n                          Request approved!{\" \"}\n                        </Card.Header>\n                        <Card.Img\n                          id=\"reqProdImg\"\n                          variant=\"bottom\"\n                          src={requestedProduct.listingProduct.image}\n                        />\n                        <Card.Body className=\"bg-secondary fw-bold\">\n                          <Card.Title className=\"text-center fw-bold\">\n                            {requestedProduct.listingProduct.name}\n                          </Card.Title>\n                          {/* <Card.Title>Posted by {requestedProduct.listingProduct.user.FirstName}</Card.Title> */}\n                        </Card.Body>\n                        <Card.Footer as=\"h6\" className=\"text-center\">\n                          Requested on {requestedProduct.dateRequested}\n                          <br/> for {requestedProduct.duration}\n                        </Card.Footer>\n                      </Card>\n                    </Col>\n                  </>\n                );\n              } else if (\n                requestedProduct.approved === false &&\n                requestedProduct.active === false\n              ) {\n                return (\n                  <>\n                    <Col>\n                      <Card\n                        border=\"danger\"\n                        id=\"reqProdCard\"\n                        style={{ width: \"22rem\" }}\n                        key={requestedProduct._id}\n                      >\n                        <Card.Header\n                          as=\"h5\"\n  