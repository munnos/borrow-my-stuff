e,\n    _id,\n    price,\n    quantity\n  } = item;\n\n  const { cart } = state\n\n  const addToCart = () => {\n    const itemInCart = cart.find((cartItem) => cartItem._id === _id)\n    if (itemInCart) {\n      dispatch({\n        type: UPDATE_CART_QUANTITY,\n        _id: _id,\n        purchaseQuantity: parseInt(itemInCart.purchaseQuantity) + 1\n      });\n      idbPromise('cart', 'put', {\n        ...itemInCart,\n        purchaseQuantity: parseInt(itemInCart.purchaseQuantity) + 1\n      });\n    } else {\n      dispatch({\n        type: ADD_TO_CART,\n        product: { ...item, purchaseQuantity: 1 }\n      });\n      idbPromise('cart', 'put', { ...item, purchaseQuantity: 1 });\n    }\n  }\n\n  return (\n    <Container fluid>    \n      <Card style={{ width: '13rem'}}>\n      <Link to={`/products/${_id}`}>\n        <Card.Img\n        variant=\"top\"\n          alt={name}\n          src={`/images/${image}`}\n        />\n        \n        <p>Item name: {name}</p>\n      </Link>\n      \n      <div>\n        {/* <div>{quantity} {pluralize(\"item\", quantity)} in stock</div> */}\n        <span>Cost: Â£{price}</span>\n      </div>\n      <Button variant=\"outline-success\" onClick={addToCart}>Add to cart</Button>\n     \n      </Card>      \n    </Container>\n  );\n}\n\nexport default ProductItem;\n\n\n",{"ruleId":"204","replacedBy":"205"},{"ruleId":"206","replacedBy":"207"},{"ruleId":"208","severity":1,"message":"209","line":7,"column":10,"nodeType":"210","messageId":"211","endLine":7,"endColumn":14},{"ruleId":"208","severity":1,"message":"212","line":2,"column":10,"nodeType":"210","messageId":"211","endLine":2,"endColumn":16},{"ruleId":"208","severity":1,"message":"213","line":2,"column":18,"nodeType":"210","messageId":"211","endLine":2,"endColumn":23},{"ruleId":"208","severity":1,"message":"209","line":5,"column":10,"nodeType":"210","messageId":"211","endLine":5,"endColumn":14},{"ruleId":"208","severity":1,"message":"209","line":9,"column":10,"nodeType":"210","message