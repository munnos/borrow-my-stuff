}\n               //reset form fields\n               setForm({name: '', description: '', image:'', category: '', duration: ''});\n\n        }\n        //console.log(token);\n        if (!token) {\n        return false;}\n    }\n        \n    \n    \n    return (       \n       \n        <div>\n        <h1>List an Item</h1>\n            <Container className=\"fluid\"> \n            <Form>\n                \n                <Form.Group>\n                <Form.Label>Enter a title for your listing:</Form.Label>\n                <Form.Control name=\"title\" type=\"text\" \n                            placeholder=\"Enter title \"\n                            value= {form.name}\n                            onChange={e=>setField('name', e.target.value)}\n                            \n                             />\n                </Form.Group>\n                \n                <Form.Group>\n                <Form.Label>Enter a description:</Form.Label>\n                <Form.Control name=\"description\" as=\"textarea\" \n                            placeholder=\"Add description \"\n                            value={form.description}\n                            onChange={e=>setField('description', e.target.value)} />   \n                            \n                </Form.Group>\n\n                <Form.Group >\n                <Form.Label>Enter image link:</Form.Label>\n                <Form.Control name=\"image\" type=\"text\" \n                            placeholder=\"Enter your image link \"\n                            value={form.image}                          \n                            onChange={e=>setField('image', e.target.value)}  />\n                </Form.Group>\n\n                \n                <Form.Group controlId>\n                    <Form.Label>Select a category for your listing</Form.Label>\n                    <Form.Select \n                        value={form.category}                    \n                         placeholder=\"Select category\"\n                         onChange={e => {setField('_id', e.target.value)}}>\n                        <option>Select Category</option>\n                        {categories.map((option, index) => (\n                        <option key={index} value={option._id}>{option.name}</option>))}                    \n                        \n                    </Form.Select>\n                </Form.Group>\n\n                <Form.Group controlId>\n                    <Form.Label>Select a listing duration:</Form.Label>\n                    <Form.Select \n                        value={form.duration}                    \n                         placeholder=\"Listing Duration\"\n                         onChange={e=> {setField('duration', e.target.value)}}>\n                           <option>Select duration </option>\n                            <option value=\"1 week\">1 week </option>\n                            <option value=\"2 week\">2 week </option>\n                            <option value=\"3 week\">3 week </option>\n                            <option value=\"1 Month\">1 Month </option>\n                            <option value=\"2 Month\">2 Month </option>\n                            <option value=\"3 Month\">3 Month </option>\n                            <option value=\"4 Month\">4 Month </option>\n                            <option value=\"5 Month\">5 Month </option>\n                            <option value=\"Indefinitely\">Indefinitely </option>\n                            <option value=\"Contact Me\">Contact Me </option>\n                    </Form.Select>\n                </Form.Group>                \n\n               <Form.Group>\n                <Button variant=\"outline-success\" className=\"mt-3\"  onClick={handleSubmit} type=\"submit\">\n                Click to list your Item\n                </Button>\n                </Form.Group>\n            </Form>\n        <Row>\n            <Card className=\"text-center bg-secondary text-white my-5 py-4\">\n            <Card.Body>\n            <p>Please upload an image to an image hosting website and paste the link in the form above - we'll link it to your listed item.  </p>\n            <Button ClassName=\"mb-3\" className=\"button\" variant=\"outline-success\" href=\"https://imgbb.com/\" target=\"_blank\" >Free image hosting site</Button>\n            </Card.Body>\n            </Card>\n        </Row> \n    </Container>\n</div>\n        \n      \n    )};\n      \n  \n  \n  export default ListItemForm;\n\n\n// {[{_id: 1, name: 'test'}].map(e => <option value={e._id}>{e.name}</option>)}","C:\\Users\\munno\\OneDrive\\Desktop\\borrow-my-stuff\\client\\src\\components\\ProductItem\\index.js",["203","204","205","206"],"import React from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { pluralize } from \"../../utils/helpers\"\nimport { useStoreContext } from \"../../utils/GlobalState\";\nimport { ADD_TO_CART, UPDATE_CART_QUANTITY } from \"../../utils/actions\";\nimport { idbPromise } from \"../../utils/helpers\";\nimport { Button, Card, Container, Row, Col } from 'react-bootstrap'; \n\nfunction ProductItem(item) {\n  const [state, dispatch] = useStoreContext();\n\n  const {\n    image,\n    name,\n    _id,\n    price,\n    quantity\n  } = item;\n\n  const { cart } = state\n\n  const addToCart = () => {\n    const itemInCart = cart.find((cartItem) => cartItem._id === _id)\n    if (itemInCart) {\n      dispatch({\n        type: UPDATE_CART_QUANTITY,\n        _id: _id,\n        purchaseQuantity: parseInt(itemInCart.purchaseQuantity) + 1\n      });\n      idbPromise('cart', 'put', {\n        ...itemInCart,\n        purchaseQuantity: parseInt(itemInCart.purchaseQuantity) + 1\n      });\n    } else {\n      dispatch({\n        type: ADD_TO_CART,\n        product: { ...item, purchaseQuantity: 1 }\n      });\n      idbPromise('cart', 'put', { ...item, purchaseQuantity: 1 });\n    }\n  }\n\n  return (\n    <Container fluid>    \n      <Card style={{ width: '13rem'}}>\n      <Link to={`/products/${_id}`}>\n        <Card.Img\n        variant=\"top\"\n          alt={name}\n          src={`/images/${image}`}\n        />\n        \n        <p>Item name: {name}</p>\n      </Link>\n      \n      <div>\n        {/* <div>{quantity} {pluralize(\"item\", quantity)} in stock</div> */}\n        <span>Cost: Â£{price}</span>\n      </div>\n      <Button variant=\"outline-success\" onClick={addToCart}>Add to cart</Button>\n     \n      </Card>      \n    </Container>\n  );\n}\n\nexport default ProductItem;\n\n\n","C:\\Users\\munno\\OneDrive\\Desktop\\borrow-my-stuff\\client\\src\\components\\CartItem\\index.js",[],"C:\\Users\\munno\\OneDrive\\Desktop\\borrow-my-stuff\\client\\src\\components\\ListingCategories\\index.js",["207"],"import React, { useState } from \"react\";\nimport { Button, Col, Container, Row} from \"react-bootstrap\";\nimport \"./style.css\";\nimport Card from \"react-bootstrap/Card\";\nimport { Link } from \"react-router-dom\";\n\n\nconst ListingCategories = (props) => {\n  // console.log(props.products.getListedProductsByCategory.category.name);\n  console.log(props);\n\n  if (!props.products.getListedProductsByCategory.length) {\n    console.log(props.products.length);\n\n    return <h1> No products listed for this category yet!</h1>;\n  } else if (props.products.getListedProductsByCategory.length)\n    console.log(props.products.getListedProductsByCategory[0].category.name);\n  return (\n    <Container fluid>\n      <Card className=\"text-center bg-secondary text-white my-5 py-4\">\n        <Card.Body>\n          <h3>{props.products.getListedProductsByCategory[0].category.name}</h3>\n        </Card.Body>\n      </Card>\n      <Row className=\"\">\n        <Col sm={4}>\n      {props.products.getListedProductsByCategory &&\n        props.products.getListedProductsByCategory.map((product) => (\n          <Card className=\"text center bg-light text-black\"\n            key={product}\n            // border=\"dark\"\n   