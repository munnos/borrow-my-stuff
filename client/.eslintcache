n variant=\"secondary\" onClick={handleClick} value=\"listed\" size=\"lg\" style={{backgroundColor:'#253F41'}}>\n              Listed items\n            </Button>\n            <Button variant=\"secondary\" onClick={handleClick} value =\"liked\" size=\"lg\" style={{backgroundColor:'#253F41'}}>\n              Liked items\n            </Button>\n           \n            <Button variant=\"secondary\" href=\"/orderHistory\"size=\"lg\" style={{backgroundColor:'#253F41'}}>\n              View order history \n            </Button>\n              \n          </div>\n        </Col>\n        <Col sm={8}>\n          {/* components are called here */}\n\n        {profileView && testing(profileView)}\n\n\n        </Col>\n\n\n\n\n      </Row>\n       {testing}\n      <Cart />\n      \n    </div>\n  );\n};\n\nexport default UserDash;","C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\utils\\GlobalState.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\components\\Liked.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\components\\Navbar\\index.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\components\\Footer\\index.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\utils\\queries.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\utils\\actions.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\utils\\auth.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\utils\\helpers.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\components\\Requested.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\components\\Listed.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\utils\\reducers.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\utils\\mutati