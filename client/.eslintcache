eed logged in user id\n//user query for name - could be stored in local storage from shop functionality\n\n\nconst UserDash = () => {\n\n  const [profileView, setProfileView] = useState(\"\");\n\n  const handleClick = (event) => {\n    event.preventDefault();\n    setProfileView(event.target.value);\n\n    console.log('value is:', event.target.value);\n  \n    // 👇️ value of input field\n    console.log('handleClick 👉️',  event.target.value);\n    //testing(event.target.value);\n  }\n\n   const testing = (profileView) =>{\n    console.log('profileView:', profileView)\n      if(profileView === \"requested\") {\n        return   <Requested profileView={profileView}/>\n      } \n\n      if (profileView === \"listed\"){\n        return <Listed profileView={profileView}/>\n      } \n\n      if (profileView === \"liked\" ){\n        return  <Liked profileView={profileView}/>\n      } \n\n        console.log(\"no matching condition\");\n      }   \n\n    // switch(profileView) {\n    //   case \"requested\":\n    //     console.log(`inside switch statement ${profileView}`);\n    //     return <Requested profileView={profileView}/>\n    //     case \"listed\":\n    //       return <Listed profileView={profileView}/> \n    //       case \"liked\":\n    //         return <Liked profileView={profileView}/> \n    //         default:\n    //           console.log(\"not match\")\n    //           return;\n      \n    \n\n   \n\n  return (\n    \n    <div className=\"container\">\n    <Row>\n        <Card className=\"text-center bg-secondary text-white my-5 py-4\">\n          <Card.Title>Welcome to your Dashboard{/*  {user.name}*/}</Card.Title> \n        </Card>\n      </Row> \n\n      <Row className=\"px-4 my-2\">\n        <Col sm={3}>\n          <div className=\"d-grid gap-2\">\n            <Button variant=\"primary\" onClick={handleClick} value=\"requested\" size=\"lg\"style={{backgroundColor:'#253F41'}}>\n              Requested items\n            </Button>\n            <Button variant=\"secondary\" onClick={handleClick} value=\"listed\" size=\"lg\" style={{backgroundColor:'#253F41'}}>\n              Listed items\n            </Button>\n            <Button variant=\"secondary\" onClick={handleClick} value =\"liked\" size=\"lg\" style={{backgroundColor:'#253F41'}}>\n              Liked items\n            </Button>\n           \n            <Button variant=\"secondary\" href=\"/orderHistory\"size=\"lg\" style={{backgroundColor:'#253F41'}}>\n              View order history \n            </Button>\n              \n          </div>\n        </Col>\n        <Col sm={8}>\n          {/* components are called here */}\n\n        {profileView && testing(profileView)}\n\n\n        </Col>\n\n\n\n\n      </Row>\n       {testing}\n      <Cart />\n      \n    </div>\n  );\n};\n\nexport default UserDash;","C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\utils\\GlobalState.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\components\\Liked.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\components\\Navbar\\index.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\components\\Footer\\index.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\utils\\queries.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\utils\\actions.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\utils\\auth.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\utils\\helpers.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\components\\Requested.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\components\\Listed.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\utils\\reducers.js",[],"C:\\SkyDrive\\Documents\\coding.bootcamp\\borrow-my-stuff\\client\\src\\utils\\mutati